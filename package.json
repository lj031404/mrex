    "start": "npm run env -s && npm run generate:gitinfo && node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng serve --ssl true --configuration=qa --aot --proxy-config proxy.conf.js",
    "start:prod": "npm run env -s && npm run generate:gitinfo && node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng serve --ssl true --configuration=production --aot --proxy-config proxy.conf.js",
    "start:local": "npm run env -s && npm run generate:gitinfo && node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng serve --ssl true --configuration=local --aot --proxy-config proxy.conf.js",
    "start:qa": "npm run env -s && npm run generate:gitinfo && node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng serve --ssl true --configuration=qa --aot --proxy-config proxy.conf.js",
    "serve:sw": "npm run build:prod -s && npx http-server ./dist -p 4200",
    "serve:prod": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng serve --prod",
    "lint": "ng lint && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    "test": "npm run env -s && ng test",
    "test:ci": "npm run env -s && npm run lint -s && ng test --code-coverage --watch=false",
    "e2e": "npm run env -s && ng e2e",
    "extract-translations": "ngx-translate-extract --input ./src/app --output ./src/translations/en-US.json ./src/translations/fr-CA.json --sort --format json --marker _translate",
    "env": "ngx-scripts env npm_package_version",
    "run:emulator:android": "cd cordova && cordova run android --emulator",
    "run:device:android": "cd cordova && cordova run android --device",
    "build:run:android": "npm run build:prod && npm run run:device:android",
    "build:release:android": "npm run build:prod && npm run copy:dist:www && cd cordova && cordova platform add android && cordova build --release android",
    "build:relase:android:local": "npm run build:local && cd cordova && cordova build --release android",
    "build:ios": "npm run build:prod && npm run copy:dist:www && cd cordova && cordova build --release ios",
    "install-apk": "adb install cordova/platforms/android/app/build/outputs/apk/release/app-release-signed.apk",
    "generate": "ng generate",
    "generate:gitinfo": "sh scripts/version.sh",
    "resources:generate": "cd cordova && cordova-res",
    "copy:dist:www": "rm -rf ./www && cp -R dist/. www/",
    "cordova:prepare:android": "npm run build:prod && npm run copy:dist && cd cordova && cordova prepare android",
    "cordova:run:emulator:android": "npm run build:prod && npm run copy:dist && cd cordova && cordova prepare android && cordova emulate android",
    "backup:ios": "sh backup_ios.sh",
    "build:env": "npm run env -s && npm run generate:gitinfo && node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build --base-href ./ --configuration=$BUILD_ENV --extract-css=false --prod --aot",
    "build": "npm run build:prod && npm run copy:dist:www && npm run send:sourcemap && npm run incrementBuild",
    "send:sourcemap": "cd scripts && sh build-android-sourcemap-release.sh",
    "incrementBuild": "cordova-update-config --appversion $(cat package.json | jq '.version' | xargs)"
  },
