    "build:prod": "npm run env -s && npm run generate:gitinfo && node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build --base-href ./ --configuration=production --extract-css=false --prod --aot",
    "build:qa": "npm run env -s && npm run generate:gitinfo && node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build --base-href ./ --configuration=qa --extract-css=false --aot",
